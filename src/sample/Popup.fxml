      <TextField fx:id="nameField" GridPane.columnIndex="1" />
      <TextField fx:id="areaField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="addressField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="phoneField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Slider fx:id="slider" prefHeight="96.0" prefWidth="241.0" scaleX="0.5" scaleY="0.5" showTickMarks="true" GridPane.columnIndex="1" GridPane.rowIndex="6">
         <GridPane.margin>
