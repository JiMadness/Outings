      <Label text="Name:" />
      <Label text="Area:" GridPane.rowIndex="1" />
      <Label text="Address:" GridPane.rowIndex="2" />
      <Label text="Phone Number:" GridPane.rowIndex="3" />
      <Label text="Minimum Charge:" GridPane.rowIndex="4" />
      <Label text="Rating:" GridPane.rowIndex="5" />
